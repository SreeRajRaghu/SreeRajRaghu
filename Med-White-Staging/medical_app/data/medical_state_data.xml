        <record id="medical_state_arrived" model="medical.state">
            <field name="name">arrived</field>
            <field name="sequence">3</field>
            <field name="s_color">rgb(80,128,80)</field>
        </record>

        <record id="medical_state_late" model="medical.state">
            <field name="name">late</field>
            <field name="sequence">4</field>
            <field name="s_color">rgba(255,0,0,1)</field>
        </record>

        <record id="medical_state_in" model="medical.state">
            <field name="name">in</field>
            <field name="sequence">5</field>
            <field name="s_color">rgba(171,142,142,0.38)</field>
        </record>

        <record id="medical_state_out" model="medical.state">
