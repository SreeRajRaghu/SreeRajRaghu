<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="1">
        <record id="medical_state_draft" model="medical.state">
            <field name="name">draft</field>
            <field name="sequence">1</field>
            <field name="s_color">rgb(128,128,128)</field>
        </record>

        <record id="medical_state_confirmed" model="medical.state">
            <field name="name">confirmed</field>
            <field name="sequence">2</field>
            <field name="s_color">rgb(0,128,0)</field>
