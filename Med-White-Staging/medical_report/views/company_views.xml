<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_company_form_inherit_medical_report" model="ir.ui.view">
        <field name="name">res.company.form.inherit.medical.report</field>
        <field name="model">res.company</field>
        <field name="inherit_id" ref="base.view_company_form"/>
        <field name="arch" type="xml">
            <notebook position="inside">
                <page string="Reporting 2">
                    <group>
                        <field name="logo2" widget="image" class="oe_avatar"/>
                        <field name="report_qr_code" widget="image" class="oe_avatar"/>
                    </group>
                </page>
            </notebook>
        </field>
    </record>
</odoo>
